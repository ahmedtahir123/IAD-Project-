@model SadsPakistan.Models.CoOrporate



<body style="width:100%;height:100%;background-color:rgb(255,255,255);">
    <section >
        <div class="container" >
            <br />
            <br />
            <br />
            <br />
            <br />


            @using (@Html.BeginForm("coorpoarte", "Home", FormMethod.Post))
            {
                <div class="form-horizontal" style="width:644px;">
                    <div class="form-group">
                        <label class="col-sm-2" style="margin:4% 0 1% 0;">Name </label>
                        <div class="col-sm-10"><input class="form-control" type="text" name="Name" placeholder="Enter Your Name" style="border-color:#f15622;border-radius:8px;width:507px; " required></div>
                    </div>
                </div>
                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="col-sm-2">E-mail</label>
                        <div class="col-sm-10"><input class="form-control" name="Email" placeholder="Enter Your E-mail" style="border-color:#f15622;border-radius:8px;width:507px;" required></div>
                    </div>
                </div>
                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="col-sm-2">Contact</label>
                        <div class="col-sm-10" style="/*border-radius:20px;*/"><input class="form-control" name="Contact" placeholder="Enter Your Contact Number" required style="border-radius:20px;border-color:#f15622;border-radius:8px;width:507px;"></div>
                    </div>
                </div>
                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="d-inline col-sm-2" style="width:180px;">WHAT IS IT REGARDING? &nbsp;<br></label>
                        <div class="col-sm-10" style="/*border-radius:20px;*/">


                      

                            <select name="Regard" class="form-control" style="border-color:#f15622;border-radius:8px;width:507px;">
                                <option   value="Donation Pickup" selected="">
                                    Donation Pickup
                                </option>
                                <option  value="Society Collection Drives">Society Collection Drives</option>
                                <option  value="Volunteer Opportunities">
                                    Volunteer Opportunities
                                </option>
                                <option  value="Others">Others</option>
                            </select>
                        </div>
                    </div>
                </div>
                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="col-sm-2">Subject</label>
                        <div class="col-sm-10" style="/*border-radius:20px;*/"><textarea name="Subject" class="form-control" style="border-color:#f15622;border-radius:8px;height:104px;width:507px;"></textarea></div>
                    </div>
                </div>
                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="col-sm-2">Query</label>
                        <div class="col-sm-10" style="/*border-radius:20px;*/"><textarea name="Quere" required class="form-control" style="border-color:#f15622;border-radius:8px;height:128px;width:507px;"></textarea></div>
                    </div>
                </div><input class="btn btn-primary" type="submit" style="background-color:rgb(239,71,71);border-color:rgb(239,71,71);width:91px;margin:0 0 0 67%;border-radius:25px;height:43px;" value="Send">
            }
        </div>
    </section>
    <script src="assets/js/jquery.min.js"></script>
    <script src="assets/bootstrap/js/bootstrap.min.js"></script>
    <script src="assets/js/UnderSpeed---Floating-Label.js"></script>
    <script src="assets/js/ebs-bootstrap-datepicker.js"></script>
    <script src="assets/js/Fixed-navbar-starting-with-transparency.js"></script>
    <script src="assets/js/PHP-Contact-Form-dark.js"></script>
    <script src="assets/js/PHP-Contact-Form-dark.js"></script>
</body>